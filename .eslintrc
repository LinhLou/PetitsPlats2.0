{
  "root":true,
  "parser":"@babel/eslint-parser",
  "parserOptions":{
    "ecmaVersion": 2018,
    "sourceType": "module",
    "requireConfigFile":false
  },
  "rules":{
    "no-undef":0,
    "no-unused-vars":0
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended"
  ]
}